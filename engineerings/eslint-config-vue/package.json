{
  "name": "@we-mobius/eslint-config-vue",
  "type": "module",
  "version": "0.0.1",
  "keywords": [
    "mobius",
    "eslint",
    "eslintconfig",
    "eslint-config",
    "vue"
  ],
  "typings": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "exports": {
    ".": {
      "require": "./dist/main.cjs",
      "import": "./dist/main.js",
      "default": "./dist/main.js"
    },
    "./index": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./ts": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "index": ["./src/index.ts"],
      "ts": ["./src/index.ts"]
    }
  },
  "scripts": {
    "build": "pnpm exec tsx ./scripts/build.ts"
  },
  "files": [
    "release/**/*",
    "dist/**/*",
    "build/**/*",
    "src/**/*",
    "scripts/**/*",
    "public/**/*",
    "docs/**/*",
    "bin/**/*",
    "typings/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "peerDependencies": {
    "eslint": ">= 8.0.0"
  },
  "dependencies": {
    "@we-mobius/eslint-config-typescript": "workspace:*",
    "eslint-plugin-vue": "^9.6.0",
    "vue-eslint-parser": "^9.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.8.3",
    "esbuild": "^0.15.10",
    "tsx": "^3.9.0",
    "vue": "^3.2.40"
  }
}
