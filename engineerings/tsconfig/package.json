{
  "name": "@we-mobius/tsconfig",
  "version": "0.0.3",
  "keywords": [
    "mobius",
    "tsconfig"
  ],
  "main": "./tsconfig.base.json",
  "exports": {
    "./tsconfig": "./tsconfig.base.json",
    "./tsconfig/base": "./tsconfig.base.json",
    "./tsconfig/monorepo": "./tsconfig.monorepo.json",
    "./tsconfig/commonjs": "./tsconfig.commonjs.json"
  },
  "files": [
    "./tsconfig.base.json",
    "./tsconfig.monorepo.json",
    "./tsconfig.commonjs.json",
    "release/**/*",
    "dist/**/*",
    "build/**/*",
    "src/**/*",
    "scripts/**/*",
    "public/**/*",
    "docs/**/*",
    "bin/**/*",
    "typings/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "peerDependencies": {
    "@types/node": "*"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  }
}
