{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "type-checking",
  "root": "engineerings/type-checking",
  "sourceRoot": "engineerings/type-checking/src",
  "tags": ["type:utility"],
  "implicitDependencies": [],
  "namedInputs": {},
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "inputs": [
        {"runtime": "node -v"},
        {"input": "dev", "projects": "self"},
        {"input": "all", "projects": "dependencies"}
      ],
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": [{"target": "build", "projects": "dependencies"}],
      "options": {
        "cwd": "engineerings/type-checking",
        "commands": ["pnpm run build"],
        "parallel": false
      }
    }
  }
}
