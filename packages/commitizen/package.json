{
  "name": "@we-mobius/commitizen",
  "type": "module",
  "version": "0.0.7",
  "private": false,
  "keywords": [
    "mobius",
    "git",
    "commit",
    "commitizen"
  ],
  "typings": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./index": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./main": {
      "require": "./dist/main.cjs",
      "import": "./dist/main.js",
      "default": "./dist/main.js"
    },
    "./adapter": {
      "require": "./dist/adapter.cjs",
      "import": "./dist/adapter.js",
      "default": "./dist/adapter.js"
    },
    "./commitlint": {
      "require": "./dist/commitlint.cjs",
      "import": "./dist/commitlint.js",
      "default": "./dist/commitlint.js"
    },
    "./ts": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "index": [
        "./src/index.ts"
      ],
      "ts": [
        "./src/index.ts"
      ]
    }
  },
  "bin": {
    "commit": "./bin/commit.js",
    "cz": "./bin/cz.cjs"
  },
  "scripts": {
    "build": "pnpm exec tsx ./scripts/build.ts",
    "type-check": "pnpm exec tsx ./scripts/type-check.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "files": [
    "release/**/*",
    "dist/**/*",
    "build/**/*",
    "src/**/*",
    "scripts/**/*",
    "public/**/*",
    "docs/**/*",
    "bin/**/*",
    "typings/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "commitizen": "^4.3.0",
    "inquirer": "^9.2.17"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.12.7",
    "@we-mobius/eslint-config": "workspace:^",
    "@we-mobius/tsconfig": "workspace:^",
    "@we-mobius/type-check": "workspace:*",
    "esbuild": "^0.20.2",
    "tsx": "^4.7.2"
  }
}
