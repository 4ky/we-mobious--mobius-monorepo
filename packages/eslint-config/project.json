{
  "name": "eslint-config",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/eslint-config/src",
  "tags": [],
  "implicitDependencies": ["!tsconfig", "!type-check"],
  "namedInputs": {},
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "inputs": [
        {
          "runtime": "node -v"
        },
        {
          "input": "dist-none"
        },
        {
          "input": "dist-only",
          "dependencies": true
        }
      ],
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ],
      "options": {
        "cwd": "packages/eslint-config",
        "commands": ["pnpm run build"],
        "parallel": false
      },
      "cache": true
    },
    "type-check": {
      "executor": "nx:run-commands",
      "inputs": [
        {
          "runtime": "node -v"
        },
        {
          "input": "dist-none"
        },
        {
          "input": "dist-none",
          "dependencies": true
        }
      ],
      "outputs": [],
      "dependsOn": [],
      "options": {
        "cwd": "packages/eslint-config",
        "commands": ["pnpm run type-check"],
        "parallel": true
      },
      "cache": true
    }
  }
}
