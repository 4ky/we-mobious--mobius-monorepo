{
  "name": "@we-mobius/mobius",
  "type": "module",
  "version": "0.0.3",
  "private": false,
  "keywords": [
    "mobius"
  ],
  "typings": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./index": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./main": {
      "require": "./dist/main.cjs",
      "import": "./dist/main.js",
      "default": "./dist/main.js"
    },
    "./ts": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "index": [
        "./src/index.ts"
      ],
      "ts": [
        "./src/index.ts"
      ]
    }
  },
  "scripts": {
    "build": "pnpm exec tsx ./scripts/build.ts",
    "type-check": "pnpm exec tsx ./scripts/type-check.ts",
    "test": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "files": [
    "release/**/*",
    "dist/**/*",
    "build/**/*",
    "scripts/**/*",
    "specs/**/*",
    "src/**/*",
    "public/**/*",
    "docs/**/*",
    "bin/**/*",
    "typings/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@we-mobius/tsconfig": "workspace:^",
    "@we-mobius/type-check": "workspace:*",
    "esbuild": "^0.20.2",
    "tsx": "^4.7.2",
    "vitest": "^1.5.0"
  }
}
