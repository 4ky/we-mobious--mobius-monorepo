{
  "name": "@we-mobius/type-check",
  "type": "module",
  "version": "0.0.2",
  "private": false,
  "description": "Type-checking support.",
  "author": {
    "name": "kongxiangyan",
    "email": "kongxiangyan@planetmatrix.tech",
    "url": "https://cigaret.world/"
  },
  "license": "GPL-3.0",
  "keywords": [
    "mobius",
    "javascript",
    "typescript",
    "type-check",
    "type-checking"
  ],
  "typings": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./index": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./ts": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "index": [
        "./src/index.ts"
      ],
      "ts": [
        "./src/index.ts"
      ]
    }
  },
  "scripts": {
    "build": "pnpm exec tsx ./scripts/build.ts",
    "type-check": "pnpm exec tsx ./scripts/type-check.ts",
    "test": "pnpm exec tsx ./test/test.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "files": [
    "release/**/*",
    "dist/**/*",
    "build/**/*",
    "src/**/*",
    "scripts/**/*",
    "public/**/*",
    "docs/**/*",
    "bin/**/*",
    "typings/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@we-mobius/eslint-config": "workspace:^",
    "@we-mobius/tsconfig": "workspace:^",
    "esbuild": "^0.19.11",
    "tsx": "^4.7.0",
    "typescript": "*"
  }
}
